{
  "name": "@eficy/reactive",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A modern annotation-based reactive state management library with MobX-compatible API, powered by @preact/signals-core",
  "keywords": [
    "reactive",
    "signal",
    "state-management",
    "annotation",
    "typescript",
    "mobx",
    "mobx-compatible",
    "observable",
    "computed",
    "action",
    "preact",
    "signals-core"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "unpkg": "./dist/index.iife.js",
  "jsdelivr": "./dist/index.iife.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.mts"
    },
    "./annotation": {
      "import": "./dist/annotation.mjs",
      "require": "./dist/annotation.js",
      "types": "./dist/annotation.d.mts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaobebe/eficy.git",
    "directory": "packages/reactive"
  },
  "bugs": {
    "url": "https://github.com/xiaobebe/eficy/issues"
  },
  "homepage": "https://github.com/xiaobebe/eficy#readme",
  "engines": {
    "npm": ">=3.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run test && npm run build"
  },
  "dependencies": {
    "@preact/signals-core": "^1.5.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "esbuild-decorators": "^0.2.4",
    "tsdown": "^0.13.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}