{
  "name": "@eficy/core-jsx",
  "version": "0.1.0",
  "description": "Eficy Core V3 - Modern React-based component system with signals reactivity",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "eficy",
    "react",
    "jsx",
    "signals",
    "reactive",
    "components"
  ],
  "author": "Eficy Team",
  "license": "MIT",
  "dependencies": {
    "@eficy/reactive": "workspace:~",
    "@eficy/reactive-react": "workspace:~",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "tsyringe": "^4.8.0",
    "reflect-metadata": "^0.2.2"
  }
}
