{
  "name": "@eficy/browser",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Use eficy in browser without compilation",
  "keywords": [
    "eficy",
    "react",
    "framework",
    "orchestration",
    "json-driven",
    "reactive",
    "unocss",
    "browser",
    "jsx",
    "runtime"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.iife.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.iife.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js",
      "require": "./dist/jsx-dev-runtime.iife.js"
    },
    "./browser-runtime": {
      "types": "./dist/browser-runtime.d.ts",
      "import": "./dist/browser-runtime.js",
      "require": "./dist/browser-runtime.iife.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yee94/Eficy.git",
    "directory": "packages/browser"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "eficy": "workspace:~",
    "reflect-metadata": "^0.2.1",
    "sucrase": "^3.35.0",
    "@rslib/core": "^0.11.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "jsdom": "^26.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
